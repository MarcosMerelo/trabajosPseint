Algoritmo Porcentaje
	//Entrada
	Definir N1 Como Real;
	Definir P Como Real;
	Definir T Como Real;
	Escribir "Programa que calcula el porcentaje";
	Escribir "Ingresa el número:";
	Leer N1;
	Escribir "Ingresa el porcentaje:";
	Leer P;
	//Proceso 
	T<-(N1*P)/100;
	//Salida
	Escribir "El porcentaje de:", N1," es:",T;
	
FinAlgoritmo
